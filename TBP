local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("Open me!") 

local b = w:CreateFolder("Scripts!")



b:Button("Troll booth",function() ------------------------------------------------------ TROLL BOOTH BUTTON ----------------------------------------------
	local args = {
    [1] = "Update",
    [2] = {
        ["Icon"] = "49009724",
        ["Text"] = "Click to claim this booth"
    }
}

	game:GetService("ReplicatedStorage").Booth:FireServer(unpack(args))
	
sound = Instance.new("Sound")
     sound.SoundId = "rbxassetid://6859791025"
sound.Parent=workspace
	sound.Volume = 1.5
     sound:Play()



	local Lib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/BoredStuff2/notify-lib/main/lib'),true))()
	Lib.prompt('Troll booth', 'Your booth now looks visually vacant but people cannot take it! (Unless you abandon it obviously)', 15)
end)

b:Button("TP to spawn",function() ------------------------------------------------------ TP TO SPAWN BUTTON ------------------------------------------
    
    
function teleportTO(placeCFrame)
    local plyr = game.Players.LocalPlayer;
    if plyr.Character then
    plyr.Character.HumanoidRootPart.CFrame = placeCFrame;
    end
end
teleportTO(game:GetService("Workspace")["Park Fountain"].Emitters.EmitPart.CFrame)

    local Lib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/BoredStuff2/notify-lib/main/lib'),true))()
    Lib.prompt('Wooooosh', 'The Booth Plaza fucker - You have arrived at the fountain!', 5)
end)


b:Button("Abandon booth",function() ------------------------------------------- ABANDON BOOTH BUTTON -------------------------------------------
local args = {
    [1] = "Abandon"
}

game:GetService("ReplicatedStorage").Booth:FireServer(unpack(args))

    local Lib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/BoredStuff2/notify-lib/main/lib'),true))()
    Lib.prompt('You abandonned your booth!', 'You can now claim another booth!', 5)
end)

