b:Button("Hack booths",function() ------------------------------------------- HACK BOOTH BUTTON -------------------------------------------

local Lib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/BoredStuff2/notify-lib/main/lib'),true))()
Lib.prompt('Booths are hacked!', 'All the unclaimed booths will now be claimed and have a troll face with the word "Hacked" on them.', 5)

local Lib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/BoredStuff2/notify-lib/main/lib'),true))()
Lib.prompt('Credits!', 'Created by GamerD4rker', 3)

while wait() do
for i,v in pairs(game.Workspace["Booth Blocks"]:GetChildren()) do
    if v.Board.BoothGui.BoothFrame.Description.Text == "Click to claim this booth" and v.Board.BoothGui.BoothFrame.Icon.Image == "rbxassetid://65470494" then
        fireclickdetector(v.Board.ClickDetector)
            game.ReplicatedStorage.Booth:FireServer("Update",{["Icon"] = "2597283821",["Text"] = "Hacked"})
            game.ReplicatedStorage.Booth:FireServer("Abandon")
            game.ReplicatedStorage.Booth:FireServer("Update",{["Icon"] = "2597283821",["Text"] = "Hacked"})
            game.Players.LocalPlayer.PlayerGui.MainGui.BoothCustomisationFrame.Visible = false
                wait(0.1)
            game.Players.LocalPlayer.PlayerGui.MainGui.BoothCustomisationFrame.Visible = false
elseif v.Board.BoothGui.BoothFrame.Description.Text == "Booth claimed by "..game.Players.LocalPlayer.DisplayName then
            game.ReplicatedStorage.Booth:FireServer("Abandon")
            game.Players.LocalPlayer.PlayerGui.MainGui.BoothCustomisationFrame.Visible = false
            end
        end
    end
end)


b:Button("Super idol booth",function() ------------------------------------------- SUPER IDOL BOOTH BUTTON -------------------------------------------
    
    local Lib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/BoredStuff2/notify-lib/main/lib'),true))()
    Lib.prompt('Super idol booth!', 'Your booth will now start singing super idol! - It is recommended to rejoin before clicking anymore buttons!', 10)

    local Lib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/BoredStuff2/notify-lib/main/lib'),true))()
    Lib.prompt('Credits!', 'Created by GamerD4rker', 3)
    
local function change(string)
    string2 = ""
    string = string:split("")
for i = 1, #string do
    string2 = string2..string[i]
    game.ReplicatedStorage.Booth:FireServer("Update",{["Icon"] = "7792039324",["Text"] = string2})
    wait(0.1)
end
    wait(1)
end
    claimed = false
while claimed == false do
    for i,v in pairs(game.Workspace["Booth Blocks"]:GetChildren()) do
        if v.Board.BoothGui.BoothFrame.Description.Text == "Click to claim this booth" and v.Board.BoothGui.BoothFrame.Icon.Image == "rbxassetid://65470494" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Board.CFrame
        fireclickdetector(v.Board.ClickDetector)
        claimed = true
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.SpawnLocation.CFrame
end
    end
    wait()
end
    while wait() do
        change("Super Idol的笑容")
        change("Super Idol de xiaorong")
        change("都没你的甜")
        change("dou mei ni de tian")
        change("八月正午的阳光")
